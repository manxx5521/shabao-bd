<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoshabao.bill.bd.mapper.ElementMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xiaoshabao.bill.bd.entity.Element">
        <id column="element_id" property="elementId" />
        <result column="element_type" property="elementType" />
        <result column="element_name" property="elementName" />
        <result column="element_desc" property="elementDesc" />
        <result column="params" property="params" />
        <result column="version" property="version" />
        <result column="order_no" property="orderNo" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        element_id, element_type, element_name, element_desc, params, version, order_no
    </sql>

</mapper>
